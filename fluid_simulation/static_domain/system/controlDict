FoamFile
{
    version    2.0;
    format     ascii;
    class      dictionary;
    location   "system";
    object     controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

timePrecision 7;

runTimeModifiable true;

startTime 0;

timeFormat general;

writeCompression off;

deltaT 0.005;

writePrecision 8;

writeControl timeStep;

application icoFoam;

writeFormat ascii;

startFrom startTime;

writeInterval 20;

stopAt endTime;

endTime 0.5;

purgeWrite 0;

